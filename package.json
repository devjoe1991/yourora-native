{
  "name": "mahi",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:dev": "eas build --profile development --platform all",
    "build:preview": "eas build --profile preview --platform all",
    "build:testflight": "eas build --profile testflight --platform ios",
    "build:production": "eas build --profile production --platform all",
    "update:dev": "eas update --branch development --message",
    "update:preview": "eas update --branch preview --message",
    "update:testflight": "eas update --branch testflight --message",
    "update:production": "eas update --branch production --message",
    "deploy:testflight": "eas build --profile testflight --platform ios && eas submit --platform ios --profile testflight",
    "deploy:production": "eas build --profile production --platform all && eas submit --platform all --profile production",
    "submit:testflight": "eas submit --platform ios --profile testflight",
    "submit:production": "eas submit --platform all --profile production"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/material-top-tabs": "^6.6.6",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/stack": "^6.3.11",
    "@shopify/flash-list": "^1.6.4",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.6.7",
    "email-validator": "^2.0.4",
    "expo": "~51.0.24",
    "expo-av": "~14.0.6",
    "expo-camera": "~15.0.14",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "formik": "^2.2.9",
    "ngrok": "^5.0.0-beta.2",
    "react": "18.2.0",
    "react-icons": "^4.7.1",
    "react-native": "0.74.5",
    "react-native-actions-sheet": "^0.9.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-pager-view": "^6.9.1",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.2.0",
    "react-native-tab-view": "^3.5.2",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0"
  },
  "private": true,
  "packageManager": "pnpm@10.12.4"
}
